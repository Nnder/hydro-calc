{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host 127.0.0.1 --port 3000",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "lint": "eslint . --ext js,ts,vue --ignore-pattern .nuxt --ignore-pattern .output --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"**/*.{js,ts,vue,json,md}\" --ignore-path .gitignore"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix --ignore-pattern .nuxt --ignore-pattern .output",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.9.0",
    "@i2d/nuxt-pdf-frame": "^0.6.1",
    "@mdi/font": "^7.4.47",
    "@nuxt/icon": "^1.12.0",
    "@nuxt/image": "^1.10.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/robots": "^5.2.8",
    "@nuxtjs/seo": "^3.0.1",
    "@nuxtjs/sitemap": "^7.2.9",
    "@pinia/nuxt": "^0.10.1",
    "@popperjs/core": "^2.11.8",
    "@vue/devtools-api": "^7.7.7",
    "@vueuse/nuxt": "^13.0.0",
    "nodemailer": "^7.0.5",
    "nuxt": "^4.0.3",
    "nuxt-aos": "^1.2.5",
    "nuxt-auth-sanctum": "^0.6.3",
    "nuxt-schema-org": "^5.0.5",
    "nuxt-swiper": "^2.0.0",
    "stats.js": "^0.17.0",
    "swiper": "^12.0.1",
    "three": "^0.180.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@nuxt/test-utils": "^3.17.2",
    "@nuxtjs/tailwindcss": "^6.13.2",
    "@percy/cli": "^1.30.7",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.24.0",
    "eslint-plugin-vue": "^10.0.0",
    "fluid-tailwind": "^1.0.4",
    "globals": "^16.0.0",
    "happy-dom": "^17.4.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.11",
    "vitest": "^3.1.1"
  }
}
